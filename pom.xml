<?xml version="1.0"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>panmx</groupId>
  <artifactId>panmx</artifactId>
  <packaging>jar</packaging>
  <version>1.1</version>

  <dependencies>
